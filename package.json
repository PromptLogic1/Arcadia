{
  "name": "arcadia",
  "version": "0.2.0",
  "private": true,
  "sideEffects": [
    "*.css",
    "*.scss",
    "./src/styles/**/*",
    "./src/lib/sentry-lazy.ts",
    "./src/instrumentation.ts",
    "./instrumentation*.ts"
  ],
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean": "rimraf .next dist",
    "clean:all": "rimraf .next dist node_modules && npm install",
    "validate": "npm run type-check && npm run lint && npm run format:check",
    "test:smoke": "node scripts/testing/smoke-tests.js",
    "audit": "node scripts/quality/run-audits.js",
    "audit:types": "node scripts/quality/type-safety-enforcer.js",
    "audit:zustand": "node scripts/quality/zustand-pattern-validator.js",
    "audit:services": "node scripts/quality/service-pattern-checker.js",
    "audit:imports": "node scripts/quality/import-optimizer.js",
    "pre-commit": "node scripts/quality/pre-commit-validator.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:status": "supabase status",
    "db:reset": "supabase db reset",
    "db:diff": "supabase db diff",
    "db:push": "supabase db push",
    "db:pull": "supabase db pull",
    "db:types": "supabase gen types typescript --local > types/database-generated.ts",
    "db:types:remote": "supabase gen types typescript --project-id cnotiupdqbdxxxjrcqvb > types/database-generated.ts",
    "db:migrate": "supabase migration up",
    "db:migration:new": "supabase migration new",
    "migration:validate": "node scripts/database/migration-manager.js validate",
    "migration:backup": "node scripts/database/migration-manager.js backup",
    "migration:new": "node scripts/database/migration-manager.js new",
    "migration:status": "node scripts/database/migration-manager.js status",
    "migration:deploy": "node scripts/database/migration-manager.js deploy",
    "migration:types": "node scripts/database/migration-manager.js types",
    "db:types:generate": "supabase gen types typescript --local > types/database-generated.ts",
    "seed:demo": "tsx scripts/database/seed-demo-boards.ts seed",
    "seed:demo:clear": "tsx scripts/database/seed-demo-boards.ts clear",
    "seed:demo:verify": "tsx scripts/database/seed-demo-boards.ts verify",
    "test:migration": "tsx scripts/database/test-migration.ts",
    "deps:clean": "node scripts/utilities/remove-unused-deps.js",
    "bundle:analyze": "node scripts/performance/bundle-optimizer.js",
    "bundle:optimize": "node scripts/performance/analyze-bundle.js"
